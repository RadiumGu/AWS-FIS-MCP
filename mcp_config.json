{
    "mcp_configuration": {
        "aws_fis_mcp": {
            "autoApprove": [],
            "disabled": false,
            "command": "uv",
            "args": [
                "--directory",
                "/home/ec2-user/mcp-servers/fis-mcp",
                "run",
                "aws_fis_mcp/server.py"
